{
	"name": "autism",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"resetDB": "npm run dropDB && npm run createDB && npm run migrateDB && npm run seedDB",
		"dropDB": "cd apps/autism-backend/src/database && npx sequelize-cli db:drop",
		"createDB": "cd apps/autism-backend/src/database && npx sequelize-cli db:create",
		"migrateDB": "npx typeorm-ts-node-commonjs -d apps/autism-backend/src/database/data-source.ts migration:run",
		"seedDB": "cd apps/autism-backend/src/database && npx sequelize-cli db:seed:all"
	},
	"private": true,
